{
  "name": "@arkrm/umi-father-seed",
  "version": "0.5.0",
  "author": "vdfor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arkrm/umi-father-seed.git"
  },
  "bugs": {
    "url": "https://github.com/arkrm/umi-father-seed/issues"
  },
  "homepage": "https://github.com/arkrm/umi-father-seed#readme",
  "private": false,
  "files": [
    "dist/",
    "lib/",
    "es/"
  ],
  "types": "dist/index.d.ts",
  "main": "lib/index",
  "module": "es/index",
  "scripts": {
    "start": "cross-env NODE_ENV=development father-build --watch",
    "build": "yarn run build:esm && yarn run build:cjs && yarn run build:umd",
    "build:esm": "cross-env BUILD_TYPE=esm father-build",
    "build:cjs": "cross-env BUILD_TYPE=cjs father-build",
    "build:umd": "cross-env BUILD_TYPE=umd node script/build-umd.js",
    "test": "umi-test",
    "test:coverage": "yarn run test --coverage",
    "clean": "node script/clean.js"
  },
  "peerDependencies": {
    "react": ">= 16.8.3",
    "react-dom": ">= 16.8.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@arkrm/bionic": "^1.0.9",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@umijs/test": "^3.1.1",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "father-build": "^1.17.2",
    "fs-extra": "^9.0.0",
    "lint-staged": "^10.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "yorkie": "^2.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "sideEffects": [
    "dist/*",
    "src/style/*",
    "src/**/style/*",
    "es/**/style/*",
    "lib/**/style/*"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "{config,src}/**/*.{js,jsx,ts}": [
      "eslint --fix"
    ],
    "{config,src}/**/*.{css,less,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
